<% @properties.each do |property| %>
  <%= content_tag :div, class: "category-block", id: "category-#{property.category.name}" do %>
    <div class="col-md-6">
      <%= render partial: 'partials/listing', locals: {property: property} %>
    </div>
  <% end %>
<% end %>
<div class="col-sm-offset-4 col-sm-4 paginate">
  <%= will_paginate @properties %>
</div>
